#!/bin/bash

source $( type -p bedrock-application-util )
source $( type -p bedrock-video-util )

function usage()
{
    cat <<EOF >&2
$( bedrock-app-swiss-usage calibration to-seqview seqview-to-metashape )
EOF
}

function calibration-description() { echo "given a directory, prepare a metashape project for LES calibration" }

function calibration-usage() { echo "abyss-photogrammetry calibration --input-dir=<path> [<options>]" }

function calibration-examples() { echo "" }

bedrock-app-swiss-init "$@"
bedrock-app-swiss-run "$@"
