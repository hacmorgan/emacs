#!/bin/bash

width=20
height=120
notches=4

section_height=$(( $height / $notches ))

x=0; y=0


for (( i=1; i<=$width; i++ )); do
    for (( j=1; j<=$height; j++ )); do
        echo $i $j
        if [ $i = 1 ] || [ $(( $j % $section_height )) = 0 ] && [ $i -le 5 ]; then
            grey=1
        else
            grey=0
        fi
        echo "$i,$j,$grey"
    done
done
